--// Placing Item For Mobile
    
local RS,PLR,CAM=game.ReplicatedStorage,game.Players.LocalPlayer,workspace.CurrentCamera
local UIS=game:GetService("UserInputService")
local RF=RS.Packages.Knit.Services.InventoryService.RF.Place  
local function Place(slot)
local m=PLR:GetMouse()
local ray=CAM:ScreenPointToRay(m.X,m.Y)
local p=RaycastParams.new()
p.FilterDescendantsInstances={PLR.Character,workspace.Camera}
p.FilterType=Enum.RaycastFilterType.Blacklist
local r=workspace:Raycast(ray.Origin,ray.Direction*500,p)
    if r and r.Instance:IsA("MeshPart") then
        local cf=CFrame.lookAt(r.Position,r.Position+r.Normal)
        return RF:InvokeServer(slot,cf,r.Instance)
    else
    warn("Invalid hit")
    end
end

