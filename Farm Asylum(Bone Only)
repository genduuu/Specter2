	rootPart.CFrame = CFrame.new(564.310608, 7.09729099, -707.37915, -0.278422475, -4.20614867e-08, 0.960458696, -1.07713021e-07, 1, 1.25687443e-08, -0.960458696, -9.99544838e-08, -0.278422475)
